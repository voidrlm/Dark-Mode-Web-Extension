{
  "name": "Dark Mode",
  "description": "Enables night mode by creating dark themes for websites on the fly.",
  "action": {},
  "manifest_version": 3,
  "author": "voidrlm",
  "version": "0.1",
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "48": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ]
}
